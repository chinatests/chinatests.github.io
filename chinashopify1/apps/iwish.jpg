function iwishCheckColl(){
  if($(".iwishcheck").length > 0) {
    $(".iwishcheck").each(function() {
      var iWishvId = $(this).attr("data-variant");
      var iWishpId = $(this).attr("data-product");
      if(isInWishlist(iWishpId,iWishvId)) { 
      	$(this).addClass("iwishAdded").html(iwish_added_txt_col); 
      	$(this).closest('.iwishGrid').append(wishlistAddedText);
      }
      $(this).removeClass("iwishcheck");
    });
  }
}

// Calls when product added from product page / quick view popup
function iWishAddFn(element) {
    console.log(iWishCnt);
    Wishlist2.getCount();
    if (saveToWishlistCounter > 0) {
      $(element).next('.view-wishlist').find('.view-wishlist--copy').hide();
    } else {
    	$(element).next('.view-wishlist').find('.view-wishlist--saved').hide();
    }
    $(element).next('.view-wishlist').css('opacity', 1);
    setTimeout(function() {
      $(element).next('.view-wishlist').css('opacity', 0);
    }, 3000);
    setTimeout(function() {
      $('body').addClass('js-save-to-wishlist--is-active');
    }, 5000);
    saveToWishlistCounter++;
}

